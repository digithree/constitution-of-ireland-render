{{#each items}}
	<li>
		<div class="timeline-image">
			<img class="img-circle img-responsive" 
					src="img/categories/nation.png"
					alt="">
		</div>
		<div class="timeline-panel">
			<div class="timeline-heading">
				<h4>{{this.sha}}</h4>
				<h4 class="subheading">{{this.commit.committer.name}}</h4>
			</div>
			<div class="timeline-body">
				<p class="text-muted">{{this.commit.message}}</p>
			</div>
		</div>
	</li>
{{/each}}